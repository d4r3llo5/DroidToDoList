<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.d4r3llo5.todolist" >
    <uses-sdk android:minSdkVersion="11"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ListTodos"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CheckList"
            android:label="@string/title_activity_check_list"
            android:parentActivityName=".ListTodos">
        </activity>
        <activity
            android:name=".TimedList"
            android:label="@string/title_activity_timed_list" >
        </activity>
    </application>

</manifest>
